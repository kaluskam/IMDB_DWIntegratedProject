                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH MOVIE_BUDGET AS (
	SELECT
		*,
		ROW_NUMBER() OVER (PARTITION BY movie, year(releaseDate) ORDER BY (SELECT NULL)) as num
	FROM MovieBudget.dbo.MovieBudget
)
SELECT 
	t.titleID, 	
	t.titleType,
	t.originalTitle, 
	t.isAdult, 
	ta.[language]
FROM MOVIE_BUDGET mb
JOIN IMDB.dbo.Titles t ON 
	t.originalTitle = mb.movie AND t.startYear=year(mb.releaseDate)
OUTER APPLY (
	SELECT TOP(1)
		ta.region, ta.[language]
	FROM IMDB.dbo.TitlesAkas ta
	WHERE ta.titleID = t.titleID AND ta.title=t.originalTitle AND ta.isOriginalTitle = 0
) ta
LEFT JOIN IMDB.dbo.TitleRatings tr ON
	tr.titleID = t.titleID
WHERE t.titleType in ('movie', 'tvMovie') AND mb.num = 1</property>
