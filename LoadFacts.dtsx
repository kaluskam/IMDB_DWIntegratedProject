              description="OLE DB Source"
              name="MoviesView"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[dbo].[MoviesWithBudgetInfo]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH MOVIE_BUDGET AS (
	SELECT
		*,
		ROW_NUMBER() OVER (PARTITION BY movie, year(releaseDate) ORDER BY (SELECT NULL)) as num
	FROM MovieBudget.dbo.MovieBudget
),
Movies AS (
SELECT 
	t.titleID, 	
	t.titleType,
	t.originalTitle, 
	t.isAdult, 
	mb.releaseDate,
	t.runtimeMinutes, 
	mb.productionBudget,
	mb.domesticGross,
	mb.worldwideGross, 
	year(mb.releaseDate) AS releaseYear,
	g.genreName,
	ta.region,
	tr.averageRating,
	tr.numVotes,
	Convert(date, getdate())  AS addedDate
FROM MOVIE_BUDGET mb
JOIN IMDB.dbo.Titles t ON 
	t.originalTitle = mb.movie AND t.startYear=year(mb.releaseDate)
OUTER APPLY (
	SELECT TOP(1) 
		g.genreName
	FROM dbo.TitlesGenres tg
		JOIN Genres g ON
			g.genreID = tg.genreID
	WHERE tg.titleID = t.titleID
) g
OUTER APPLY (
	SELECT TOP(1)
		ta.region
	FROM dbo.TitlesAkas ta
	WHERE ta.titleID = t.titleID AND ta.title=t.originalTitle
) ta
LEFT JOIN TitleRatings tr ON
	tr.titleID = t.titleID
WHERE t.titleType in ('movie', 'tvMovie') AND mb.num = 1)
SELECT * from Movies where averageRating IS NOT NULL AND numVotes IS NOT NULL AND runtimeMinutes IS NOT NULL</property>
